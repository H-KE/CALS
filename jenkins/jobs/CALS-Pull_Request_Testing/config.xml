      <command>#!/bin/bash

# export PATH=$PATH:/usr/local/share
# export PATH=/var/lib/jenkins/.rvm/gems/ruby-2.3.3/bin:/var/lib/jenkins/.rvm/gems/ruby-2.3.3@global/bin:/var/lib/jenkins/.rvm/rubies/ruby-2.3.3/bin:/usr/local/bin:/bin:/usr/bin:/usr/local/sbin:/usr/sbin:/sbin:/opt/aws/bin:/var/lib/jenkins/.rvm/bin:/var/lib/jenkins/.rvm/bin

export BASE_CALS_API_URL=&quot;http://localhost:3001/v1&quot;
export CALS_API_URL=&quot;https://cals-api.preint.cwds.io&quot;
export GEO_SERVICE_URL=&quot;https://geo.preint.cwds.io&quot;
export BASE_SEARCH_API_URL=&quot;https://dora.preint.cwds.io&quot;
# export RAILS_RELATIVE_URL_ROOT=&quot;/&quot;
export DISABLE_SPRING=1

env

source /etc/profile.d/rvm.sh
rvm use default

set -e

ruby -v 
echo -----------------------------------------------------
gem install bundler

bundle install 
yarn install

# rake tmp:cache:clear; rm -rf public/assets public/packs public/packs-test; rails assets:precompile RAILS_ENV=test

# bundle exec rake tmp:cache:clear
rm -rf public/assets public/packs public/packs-test
# rails assets:precompile RAILS_ENV=test

export CHROME=true

# run rspec inside container
docker-compose down
docker-compose up --build -d

sleep 5
# bin/webpack &amp;&amp; docker-compose exec -T cals bash -c &apos;RAILS_ENV=test bundle exec rspec&apos;
# bundle exec rake build_rails:bundle_yarn
bundle exec rake spec:cals

docker-compose down


#change reports folder permission 
# sudo chown -R jenkins:jenkins reports

#docker-compose up -d --force-recreate
#echo &quot;-------------------------&quot;
#echo &quot;-------------------------&quot;
#echo &quot;container up&quot;
#echo &quot;sleep&quot;
#sleep 5
#bundle exec rake build_rails:bundle_yarn
#bundle exec rake spec:cals

# run karma normally
npm run karma-ci

sleep 5</command>
    </hudson.tasks.Shell>
